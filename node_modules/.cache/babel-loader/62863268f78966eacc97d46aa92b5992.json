C,cAAA,KAAK,EAAC,YAQP;AARqB,cAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,uBAAE,QAAA,CAAA,OAAA,CAAQ,KAAR,CAAF;AAAA;AAQ1B,aARR,E;gCACA;AAAA,uBAA8C,CAA9C,mBAAA,CAA8C,KAA9C,EAA8C;AAAzC,kBAAA,KAAK,EAAC,cAAmC;AAAnB,kBAAA,GAAG,EAAE,KAAK,CAAC;AAAQ,iBAA9C,E,IAAA,E;;AAAA,kB,UAAA,CAA8C,EAC9C,mBAAA,CAKM,KALN,EAKM,IALN,EAKM,CAJJ,mBAAA,CAA8C,KAA9C,EAAA,UAAA,EAA8C,gBAAA,CAAnB,KAAK,CAAC,KAAa,CAA9C,EAAsC;AAAA;AAAtC,iBAII,EAHJ,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,mBAAA,CAA+D,MAA/D,EAAA,UAAA,EAA+D,gBAAA,CAApC,KAAK,CAAC,IAAN,CAAW,SAAX,CAAoB,CAApB,EAAoB,EAApB,CAAoC,CAA/D,EAA+C;AAAA;AAA/C,iBACI,CAFN,CAGI,CALN,CAD8C,CAA9C;AAAA,e;;;;aADA,E;;AAAA,c,WAAA,CADkE,CAApE;AAAA,W;;;;SADF,E;;AAAA,S;OAWS,CAXT,E;;AAAA,OAAwD,EAAvB;AAAA,K,CADhB;;;;AAAA,GAAnB,CAuBI,EATJ,mBAAA,CAQM,KARN,EAAA,UAAA,EAQM,CAPJ,YAAA,CAMgB,wBANhB,EAMgB;AALb,IAAA,YAAW,EAAE,IAAA,CAAA,gBAKA;AAJb,IAAA,eAAc,EAAE,QAAA,CAAA,mBAIH;AAHb,iBAAW,CAGE;AAFd,IAAA,MAAM,EAAC,mBAEO;AADb,IAAA,KAAK,EAAE,KAAA,CAAA;AACM,GANhB,E,IAAA,E;;AAAA,I,4CAAA,CAOI,CARN,CASI,CAxBN,CAyBI,CA1BN,C","sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"top\">\n      <el-row :gutter=\"20\">\n        <el-col :xs=\"24\" :sm=\"8\" :md=\"8\" v-for=\"video in videos\" :key=\"video.id\">\n          <!-- <el-card class=\"video-card\" @click.native=\"goVideo(video)\"> -->\n            <el-card class=\"video-card\" @click=\"goVideo(video)\">\n            <img class=\"video-avatar\" :src=\"video.avatar\">\n            <div>\n              <div class=\"video-title\">{{video.title}}</div>\n              <div class=\"video-bottom clearfix\">\n                <span class=\"video-info\">{{video.info.substring(0, 40)}}</span>\n              </div>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n      <div class=\"block\">\n        <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :page-size=\"6\"\n          layout=\"prev, pager, next\"\n          :total=\"total\">\n        </el-pagination>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as API from '@/api/video/';\n\nexport default {\n  name: 'home',\n  data() {\n    return {\n      videos: [],\n      start: 0,\n      limit: 6,\n      total: 0,\n    };\n  },\n  methods: {\n    handleCurrentChange(val) {\n      this.start = this.limit * (val - 1); // val 页面\n      this.load();\n    },\n    load() {\n      API.getVideos(this.start, this.limit).then((res) => {\n        this.videos = res.data.items;\n        this.total = res.data.total;\n      });\n    },\n    goVideo(video) {\n      this.$router.push({ name: 'showVideo', params: { videoID: video.id } });\n    },\n  },\n  components: {\n  },\n  beforeMount() {\n    this.load();\n  },\n  emits: ['close'],\n};\n</script>\n\n<style>\n.video-avatar {\n  width: 100%;\n}\n.video-title {\n  margin: 4px 0px 4px 0px;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  overflow: hidden;\n}\n.video-bottom {\n  margin-top: 4px;\n}\n.video-info {\n  color: #909399;\n}\n.video-card {\n  margin-bottom: 14px;\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}